{
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=Rookies;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "IdentityServerConfig": {
    "Clients": {
      "Swagger": {
        "RedirectUris": "https://waxshop.azurewebsites.net/swagger/oauth2-redirect.html",
        "PostLogoutRedirectUris": "https://waxshop.azurewebsites.net/swagger/oauth2-redirect.html",
        "AllowedCorsOrigins": "https://waxshop.azurewebsites.net"
      },
      "MVC": {
        "RedirectUris": "https://minhshop.azurewebsites.net/signin-oidc",
        "PostLogoutRedirectUris": "https://minhshop.azurewebsites.net/signout-callback-oidc"
      },
      "React": {
        "RedirectUris": "http://localhost:3000/signin-oidc",
        "PostLogoutRedirectUris": "http://localhost:3000/signout-callback-oidc",
        "AllowedCorsOrigins": "http://localhost:3000",
        "RedirectLogout": "http://localhost:3000/signout-oidc"
      }
    }
  }
}
